    "@reduxjs/toolkit": "^1.7.1",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "babel-loader": "^8.0.0-beta.2",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.0.0",
    "css-loader": "^6.5.1",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.2",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
