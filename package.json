{
  "name": "ortomorrow",
  "version": "1.0.0",
  "description": "O/R Tomorrow",
  "author": "A-Tech",
  "repository": "git+https://github.com/a-m-s-r/ortomorrow",
  "private": true,
  "scripts": {
    "build": "webpack --progress --profile -c ./config/webpack.prod.ts",
    "serve-prod": "NODE_ENV=production nodemon -w ./server ./server/server.ts",
    "start": "yarn build && yarn serve-prod",
    "dev": "concurrently \"yarn dev-client\" \"yarn dev-server\"",
    "dev-client": "webpack serve --history-api-fallback -c ./config/webpack.dev.ts",
    "dev-server": "PORT=9090 LOCAL_MONGODB=1 nodemon -w ./server ./server/server.ts",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
